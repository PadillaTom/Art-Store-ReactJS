amily: var(--FontWork);\n      font-size: 1.3rem;\n      letter-spacing: 0.7px;\n      font-weight: 300;\n      width: 85%;\n      margin: 0 auto;\n    }\n  }\n\n  @media (min-width: 1150px) {\n    grid-template-columns: 1fr 1fr;\n    place-items: center;\n    .description {\n      h1 {\n        padding: 1.2rem 0rem;\n      }\n    }\n  }\n`;\nexport default AboutPage;\n","/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Pages/ProductsPage.js",["159"],"import React from \"react\";\nimport styled from \"styled-components\";\n\nconst ProductsPage = () => {\n  return <h4>products page</h4>;\n};\n\nconst Wrapper = styled.div`\n  .products {\n    display: grid;\n    gap: 3rem 1.5rem;\n    margin: 4rem auto;\n  }\n  @media (min-width: 768px) {\n    .products {\n      grid-template-columns: 200px 1fr;\n    }\n  }\n`;\n\nexport default ProductsPage;\n","/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Pages/HomePage.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Pages/CheckoutPage.js",["160"],"import React from \"react\";\nimport styled from \"styled-components\";\nconst CheckoutPage = () => {\n  return <h4>checkout page</h4>;\n};\nconst Wrapper = styled.div``;\nexport default CheckoutPage;\n","/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/Navigation/Navbar.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/Navigation/Sidebar.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/Navigation/UserButtons.js",["161"],"import React from \"react\";\nimport styled from \"styled-components\";\nimport { Link } from \"react-router-dom\";\nimport { FaShoppingCart, FaUserMinus, FaUserPlus } from \"react-icons/fa\";\n\nimport { useProductsContext } from \"../../Context/products_context\";\n\nconst 