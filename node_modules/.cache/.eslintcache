  .content {\n      padding-top: 5%;\n    }\n    .desc {\n      padding: 0.7rem 0rem;\n    }\n    .info {\n      padding-bottom: 0.5rem;\n    }\n    .price {\n      font-size: 2rem;\n    }\n  }\n`;\n\nexport default SingleProductPage;\n","/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Pages/ErrorPage.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Utils/constants.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/Logotype/index.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/Logotype/Logotype.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/Utils/index.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/Utils/Error.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/Utils/Loading.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/Products/index.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/HomePage/index.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/HomePage/Newsletter.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/HomePage/PageHero.js",["267","268"],"import React from 'react'\nimport styled from 'styled-components'\nimport { Link } from 'react-router-dom'\nconst PageHero = () => {\n  return <h4>page hero</h4>\n}\n\nconst Wrapper = styled.section`\n  background: var(--clr-primary-10);\n  width: 100%;\n  min-height: 20vh;\n  display: flex;\n  align-items: center;\n\n  color: var(--clr-primary-1);\n  a {\n    color: var(--clr-primary-3);\n    padding: 0.5rem;\n    transition: var(--transition);\n  }\n  a:hover {\n    color: var(--clr-primary-1);\n  }\n`\n\nexport default PageHero\n","/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/HomePage/Services.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/HomePage/Hero.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/HomePage/FeaturedProducts.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Utils/helpers.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/SingleProduct/index.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/SingleProduct/ProductImages.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/SingleProduct/Stars.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/SingleProduct/AddToCart.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/SingleProduct/AmountButtons.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Context/filter_context.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Reducers/filter_reducer.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/Utils/ScrollToTop.js",["269"],"import { useEffect } from \"react\";\nimport { withRouter } from \"react-router-dom\";\n\nfunction ScrollToTop({ history }) {\n  useEffect(() => {\n    const unlisten = history.listen(() => {\n      window.scrollTo(0, 0);\n    });\n    return () => {\n      unlisten();\n    };\n  }, []);\n\n  return null;\n}\n\nexport default withRouter(ScrollToTop);\n","/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/Products/Sort.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/Products/Filters.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/Products/ProductList.js",[],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/index.js",[],["270","271"],"/Users/padillatom/PadillaTom/Tomas/Programacion/Practice Projects/React JS/Art&Store/src/Components/StripeCheckout.js",["272","273","274","275","276","277","278","279","280","281","282","283"],"import React, { useState, useEffect } from 'react'\nimport styled from 'styled-components'\nimport { loadStripe } from '@stripe/stripe-js'\nimport {\n  CardElement,\n  useStripe,\n  Elements,\n  useElements,\n} from '@stripe/react-stripe-js'\nimport axios from 'axios'\nimport { useCartContext } from '../context/cart_context'\nimport { useUserContext } from '../context/user_context'\nimport { formatPrice } from '../utils/helpers'\nimport { useHistory } from 'react-router-dom'\n\nconst CheckoutForm = () => {\n  return <h4>hello from Stripe Checkout </h4>\n}\n\nconst StripeCheckout = () => {\n  return (\n    <Wrapper>\n      <CheckoutForm />\n    </Wrapper>\n  )\n}\n\nconst Wrapper = styled.section`\n  form {\n    width: 30vw;\n    align-self: center;\n    box-shadow: 0px 0px 0px 0.5px rgba(50, 50, 93, 0.1),\n      0px 2px 5px 0px rgba(50, 50, 93, 0.1),\n      0px 1px 1.5px 0px rgba(0, 0, 0, 0.07);\n    border-radius: 7px;\n    padding: 40px;\n  }\n  input {\n    border-radius: 6px;\n    margin-bottom: 6px;\n    padding: 12px;\n    border: 1px solid rgba(50, 50, 93, 0.1);\n    max-height: 44px;\n    font-size: 16px;\n    width: 100%;\n    background: white;\n    box-sizing: border-box;\n  }\n  .result-message {\n    line-height: 22px;\n    font-size: 16px;\n  }\n  .result-me